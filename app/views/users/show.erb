<div class="row">
    <div class="box col-lg-offset-1 col-lg-10">
        <div class="col-lg-12">
            <hr>
            <h2 class="intro-text text-center">Current <strong>Collection</strong></h2>
            <hr>
            <div class="row">
              <% @pokemons.each do |p| %>
              <div class="col-md-3 col-sm-4 col-xs-6">
                <figure class="figure">
                  <a href="/pokedex/<%=p.pokedex.id%>"><img src="<%=p.pokedex.image_url%>" class="img-responsive"></a>
                  <figcaption class="figure-caption"><%=p.pokedex.name%> - <%=p.cp%></figcaption>
                </figure>
              </div>
              <%end%>
            </div>
        </div> <!-- end col-lg-12 -->
    </div> <!-- end .box -->
</div> <!-- end .row -->
